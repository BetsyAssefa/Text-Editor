{
  "name": "JATE",
  "version": "1.0.0",
  "description": "Just Another Text Editor (JATE) - A PWA",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",              
    "start:dev": "concurrently \"npm run server\" \"npm run client\"", 
    "server": "nodemon server.js",          
    "build": "cd client && npm run build", 
    "install": "npm install && cd client && npm install", 
    "client": "cd client && npm start"     
  },
  "keywords": ["PWA", "Text Editor", "JATE"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
